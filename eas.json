{
    "cli": {
        "version": ">= 12.5.1"
    },
    "build": {
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "APP_VARIANT": "production"
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "APP_VARIANT": "development"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle",
                "gradleCommand": ":app:bundleRelease"
            },
            "env": {
                "APP_VARIANT": "production"
            }
        },
        "development-static": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "APP_VARIANT": "development"
            }
        }
    }
}